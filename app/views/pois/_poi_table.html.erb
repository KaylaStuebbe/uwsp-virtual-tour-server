<table class="poi-table table table-striped">
  <tr>
    <th>Title</th>
    <th>Lat</th>
    <th>Long</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% pois.each do |poi| %>
  <tr>
    <td><%= poi.title %></td>
    <td><%= poi.lat %></td>
    <td><%= poi.long %></td>
    <td><%= link_to 'Show', tour_poi_path(@tour, poi) %></td>
    <td><%= link_to 'Edit', edit_tour_poi_path(@tour, poi) %></td>
    <td><%= link_to 'Destroy', tour_poi_path(@tour, poi), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>